@model TH05.Models.Customer
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Create</h2>

@using (Html.BeginForm("Create", "Customer", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
   
    
    <p>@Html.LabelFor(model => model.NameCus)</p>
    <p>@Html.EditorFor(model => model.NameCus)</p>
    <p>
        @Html.ValidationMessageFor(model => model.NameCus)
    </p>
    <p>@Html.LabelFor(model => model.PhoneCus)</p>
    <p>@Html.EditorFor(model => model.PhoneCus)</p>
    <p>
        @Html.ValidationMessageFor(model => model.PhoneCus)
    </p>
    <p>@Html.LabelFor(model => model.EmailCus)</p>
    <p>@Html.EditorFor(model => model.EmailCus)</p>
    <p>
        @Html.ValidationMessageFor(model => model.EmailCus)
    </p>
    
    <input type="submit" value="Create" />
}



